<destination>
	<branding>
		<logo location="destination" image="https://www.sermonaudio.com/images/sa-logo-150-2px.png" click="http://www.sermonaudio.com/webcastinfo">
			<title text="SermonAudio.com Update Test" language="EN"/>
		</logo>
	</branding>
	<channel_discovery_service url="https://www.sermonaudio.com/wirecastcd_sa.asp"/>
	<formats allow_user_defined="true">
		<format>
			<title text="HD - 1Mbps 1280x720 16:9" language="EN"/>
			<video type="flash" encoder="H.264" profile="main" width="1280" height="720" fps="30" bitrate="1000" keyframe="60" key_frame_aligned="true" absolute_time_stamps="true" strict_cbr="true"/>
			<audio encoder="aac" bitrate="64" samplerate="44100" channels="1"/>
		</format>
		<format>
			<title text="LO - 300kbps 480x270 16:9" language="EN"/>
			<video type="flash" encoder="H.264" profile="baseline" width="480" height="270" fps="30" bitrate="300" keyframe="60" key_frame_aligned="true" absolute_time_stamps="true" strict_cbr="true"/>
			<audio encoder="aac" bitrate="64" samplerate="44100" channels="1"/>
		</format>
	</formats>
	<gui show_channels="true" require_password="false">
		<input name="username" required="true">
			<title text="Stream Token ID:" language="EN"/>
		</input>
	</gui>
</destination>
